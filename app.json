{
  "expo": {
    "name": "HealthBot",
    "slug": "HealthBot",
    "scheme":"healthbot",
    "privacy": "public",
    "version": "1.0.0",
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "icon": "./assets/logo_f.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cse210group6.HealthBot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_f.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cse210group6.HealthBot"
    },
    "web": {
      "favicon": "./assets/logo.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "0a3c431e-2d40-4938-af3c-06cf7026213e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0a3c431e-2d40-4938-af3c-06cf7026213e"
    },
    "owner": "cse210-group6",
    "experiments": {
      "baseUrl": "/HealthBot"
    }
  }
}
